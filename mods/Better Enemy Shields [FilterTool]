        set GD_Shields.Skills.Roid_Shield_Skill_ThresherRaid:BehaviorProviderDefinition_0.Behavior_PostAkEvent_166 bForLocalPlayerOnly false

        set GD_Shields.Skills.Roid_Shield_Skill_ThresherRaid:BehaviorProviderDefinition_0.Behavior_PostAkEvent_167 bForLocalPlayerOnly false

        set GD_Shields.Skills.Roid_Shield_Skill_ThresherRaid:BehaviorProviderDefinition_0.Behavior_PostAkEvent_168 bForLocalPlayerOnly false

